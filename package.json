{
  "name": "trinity",
  "sideEffects": false,
  "version": "0.1.0",
  "engines": {
    "npm": "6.13.4",
    "node": "12.16.1"
  },
  "private": true,
  "scripts": {
    "start": "env NODE_ENV=production webpack-dev-server --open --config webpack.prod.js",
    "build": "env NODE_ENV=production webpack --config webpack.prod.js",
    "start-client": "env NODE_ENV=development webpack-dev-server --open --config webpack.dev.js --hot",
    "start-backend": "env NODE_ENV=development node ./node_modules/nodemon/bin/nodemon.js ./backend/bin/www",
    "dev": "node ./node_modules/concurrently/bin/concurrently.js \"npm run start-client\" \"npm run start-backend\"",
    "heroku-postbuild": "ng build --prod"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@oipwg/hdmw": "^2.0.0-beta.8",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "bip32": "^2.0.5",
    "cookie-parser": "~1.4.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "nicehash-api": "git+https://github.com/bradicalone/nicehash-api.git",
    "passport": "^0.4.1",
    "passport-coinbase-oauth2": "0.0.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "qrcode.react": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "spartanbot": "git+https://github.com/bradicalone/spartanbot.git",
    "timestring": "^6.0.0",
    "uid": "^1.0.0",
    "ws": "^7.2.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
